#include<iostream>
#include<vector>
using namespace std;

struct group
{
	string nazwa;
	vector <string> studenci;
};

void add_student(struct group p)
{
	cout << "Nazwa grupy " << p.nazwa << endl;
	cout << "Imie studenta " << p.studenci.at(0);
}

int main()
{
	struct group p;
	string nazwa;
	string student;

	cout << "Podaj nazwe grupy" << endl;
	cin >> nazwa;
	cout << "Podaj imie studenta" << endl;
	cin >> student;

	p.nazwa = nazwa;
	p.studenci.push_back(student);

	add_student(p);
	return 0;
}
