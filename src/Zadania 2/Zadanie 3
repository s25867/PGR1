#include <iostream>
#include <string>
#include <vector>
using namespace std;

auto dot_product()
{
	vector <float> W1;
	vector <float> W2;
	float liczba;
	float suma = 0;

	cout << "Podaj wartosci pierwszego wektora" << endl;
	for (int i = 0; i < 2; i++)
	{
	cin >> liczba;
	W1.push_back(liczba);
	}

	cout << "Podaj wartosci drugiego wektora" << endl;
	for (int i = 0; i < 2; i++)
	{
		cin >> liczba;
		W2.push_back(liczba);
	}

	cout << "Wynik to: " << ( W1.at(0) * W2.at(0) + W1.at(1) * W2.at(1) ) << endl;
}

int main()
{
	dot_product();
    return 0;
}
