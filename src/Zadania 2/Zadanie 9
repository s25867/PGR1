bool contains_all(struct group p)
{
	string pomocnicza;
	string wyraz;
	for (int i = 0; i < p.drugi.size(); i++) {
		wyraz = p.drugi[i];
		if (p.drugi[i] != p.pierwszy[i]) {
			cout << "Drugi wektor nie jest podzbiorem pierwszego" << endl;
			return 0;
		}
		else continue;
	}
	cout << "Drugi wektor jest podzbiorem pierwszego" << endl;
	return 0;
}

int main()
{
	struct group p;
	contains_all(p);
	return 0;
}
