#include<iostream>
#include<vector>
using namespace std;

struct group
{
	vector <string> pierwszy = { "Jan", "Julia", "Jacek", "Ola" };
	vector <string> drugi = { "Jan", "Julia", "Jacek" };
};

//funckja input
