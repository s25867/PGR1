#include <iostream>
using namespace std;

int main() {
    int liczba;
    int pomocznicza;
    int i;
    int b = 0;
    cin >> liczba;
    for (i = 2; i < liczba; i++) {
        b++;
        if (liczba % i == 0) {
            cout << "nie jest" << endl;
            break;
        }
    }
    cout << i << endl;
}
