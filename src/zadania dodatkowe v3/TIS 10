#include <iostream>
#include <vector>
#include <string>
using namespace std;

class macierze {
public:
    vector<int>liczby = {1,2,3,4,5,6,7,8};
    int rozmiar;
    int wariant;
    int pomocnicza;
    void wynik() {

        cout << "podaj wariant macierzy (1 2 3 4)" << endl;
        cin >> wariant;
        cout << "podaj rozmiar" << endl;
        cin >> rozmiar;

        switch (wariant) {
        case 1:
            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i+1 << " ";
            }
            cout << "]" << endl;

            cout << "[";

            for (int i = 0; i < rozmiar; i++) {
                cout << i + 2 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 3 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 4 << " ";
            }
            cout << "]" << endl;
            break;
        case 2:
            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i - 2 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i - 1 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 1 << " ";
            }
            cout << "]" << endl;
            break;
        case 3:
            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 2 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 1 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 3<< " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 4 << " ";
            }
            cout << "]" << endl;
            break;
        case 4:
            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 1 << " ";
            }
            cout << "]" << endl;

            cout << "[";

            for (int i = 0; i < rozmiar; i++) {
                cout << i + 2 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 3 << " ";
            }
            cout << "]" << endl;

            cout << "[";
            for (int i = 0; i < rozmiar; i++) {
                cout << i + 4 << " ";
            }
            cout << "]" << endl;
            break;

        }
    }
};


int main() {
    macierze n;
    n.wynik();

    return 0;
}
