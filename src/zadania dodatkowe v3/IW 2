#include <iostream>
using namespace std;

int main() {
    int rok;
    bool przest;
    int miesiac;
    cout << "podaj rok" << endl;
    cin >> rok;
    cout << "podaj miesiac" << endl;
    cin >> miesiac;
    if (rok % 4 == 0) {
        if (rok % 100 == 0) {
            if (rok % 400 == 0)
                przest = 1;
            else
                przest = 0;
        }
        else
            przest = 1;
    }
    else { przest = 0; }

    if (przest == 1) {
        switch (miesiac) {
        case 1:
            cout << "31";
            break;
        case 2:
            cout << "29";
            break;
        case 3:
            cout << "31";
            break;
        case 4:
            cout << "30";
            break;
        case 5:
            cout << "31";
            break;
        case 6:
            cout << "30";
            break;
        case 7:
            cout << "31";
            break;
        }
    }
        if (przest == 0) {
            switch (miesiac) {
            case 1:
                cout << "31";
                break;
            case 2:
                cout << "28";
                break;
            case 3:
                cout << "31";
                break;
            case 4:
                cout << "30";
                break;
            case 5:
                cout << "31";
                break;
            case 6:
                cout << "30";
                break;
            case 7:
                cout << "31";
                break;
            }
        }
    }
