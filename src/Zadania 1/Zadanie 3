#include <iostream>
#include <string>

int main()
{
	std::string a;
	std::string b;
	std::cout << "Podaj swoje pierwsze imie. \n";
	std::getline(std::cin, a);
	std::cout << "Podaj swoje drugie imie. \n";
	std::getline(std::cin, b);
	if (a.length() > b.length()) {
		std::cout << "Pierwsze imie jest dłuższe od drugiego! \n";
	}
	else 
		if (a.length() < b.length()) {
			std::cout << "Drugie imie jest dłuższe od pierwszego... \n";
	}
		else 
			std::cout << "Pierwsze imie i drugie są tych samych długości. \n";
	return 0;
}
